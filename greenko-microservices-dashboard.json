{
  "annotations": {
    "list": []
  },
  "editable": true,
  "panels": [
    {
      "type": "stat",
      "title": "Services UP",
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "targets": [
        {
          "expr": "sum(up{job=~\"gateway-service|asset-service|alert-service|telemetry-service\"})",
          "refId": "A"
        }
      ],
      "gridPos": { "h": 5, "w": 24, "x": 0, "y": 0 }
    },
    {
      "type": "timeseries",
      "title": "HTTP Requests Rate (req/sec)",
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "targets": [
        {
          "expr": "rate(http_server_requests_seconds_count[1m])",
          "refId": "A"
        }
      ],
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 6 }
    },
    {
      "type": "timeseries",
      "title": "JVM Memory Used (MB)",
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "targets": [
        {
          "expr": "jvm_memory_used_bytes / 1024 / 1024",
          "refId": "A"
        }
      ],
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 6 }
    },
    {
      "type": "timeseries",
      "title": "JVM CPU Usage (%)",
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "targets": [
        {
          "expr": "process_cpu_usage * 100",
          "refId": "A"
        }
      ],
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 14 }
    },
    {
      "type": "timeseries",
      "title": "Container CPU Usage",
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "targets": [
        {
          "expr": "rate(container_cpu_usage_seconds_total[1m])",
          "refId": "A"
        }
      ],
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 14 }
    },
    {
      "type": "timeseries",
      "title": "Container Memory Usage",
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "targets": [
        {
          "expr": "container_memory_usage_bytes / 1024 / 1024",
          "refId": "A"
        }
      ],
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 22 }
    }
  ],
  "schemaVersion": 38,
  "style": "dark",
  "time": {
    "from": "now-15m",
    "to": "now"
  },
  "templating": {
    "list": [
      {
        "name": "DS_PROMETHEUS",
        "type": "datasource",
        "query": "prometheus",
        "current": {
          "selected": true
        }
      }
    ]
  },
  "title": "Greenko Microservices Monitoring",
  "version": 1
}
